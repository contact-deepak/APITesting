							"});\r",
							"var jsonData = pm.response.json();\r",
							"pm.collectionVariables.set(\"id\", jsonData.data._id);   \r",
							"pm.collectionVariables.set(\"name\", jsonData.data.name);\r",
							"pm.collectionVariables.set(\"isoCode\", jsonData.data.isoCode);\r",
